        "@types/styled-components": "^5.1.3cl",
        "@types/uuid": "^8.3.0",
        "apollo-boost": "^0.4.7",
        "apollo-client": "^2.6.4",
        "apollo-link-ws": "^1.0.20",
        "apollo-utilities": "^1.3.2",
        "graphql": "^14.0.0",
        "grommet": "^2.15.0",
        "grommet-icons": "^4.5.0",
        "grommet-styles": "^0.2.0",
        "lodash": "^4.14.161",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-epic-spinners": "^0.5.0",
        "react-helmet": "^6.1.0",
        "react-promise-suspense": "^0.3.3",
        "react-router": "^5.1.5",
