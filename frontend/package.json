        "typescript": "~3.7.2",
        "uuid": "^8.3.0"
    },
    "scripts": {
        "build": "react-scripts build",
        "build-schema-fragment": "node src/schemaQuery.js",
        "eject": "react-scripts eject",
        "precommit": "lint-staged",
        "start": "react-scripts start",
        "test": "react-scripts test",
        "codegen": "apollo client:codegen --no-addTypename --target typescript --globalTypesFile=src/__generated__/globalTypes.ts --passthroughCustomScalars --customScalarsPrefix=GraphQL_ __generated__"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "lint-staged": {
        "src/**/*.{js,jsx,json,css}": [
            "prettier --single-quote --write",
            "git add"
        ]
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.2",
        "@testing-library/user-event": "^7.1.2",
        "@types/jest": "^24.0.0",
        "apollo": "^2.28.3",
        "nodemon": "^2.0.4",
        "prettier": "^2.1.2"
    }
}
